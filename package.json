{
  "name": "canvas-planner",
  "version": "0.0.1",
  "description": "UI portion of planner application for canvas-lms.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "extract": "format-message extract src/**/**/*.js -o config/locales/en.json",
    "lint-staged": "lint-staged",
    "lint": "eslint src/",
    "prepublish": "scripts/build",
    "start": "npm-run-all --parallel start:*",
    "start:json-server": "json-server --watch --port 3004 db.json",
    "start:webpack-dev": "webpack-dev-server --host 0.0.0.0 --port 3005",
    "test": "jest"
  },
  "keywords": [
    "planner",
    "canvas"
  ],
  "author": "Instructure, Inc.",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-1": "^6.22.0",
    "enzyme": "^2.8.0",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.16.1",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.21.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.0.0 || ^3.0.0 || ^4.0.0",
    "eslint-plugin-react": "^6.4.1",
    "format-message-cli": "^5.1.4",
    "jest": "^19.0.2",
    "json-server": "^0.9.6",
    "lint-staged": "^3.4.0",
    "npm-run-all": "^4.0.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "peerDependencies": {
    "instructure-ui": "^1.3.4",
    "react": "^0.14.7 || >=15.0.0",
    "react-dom": "^0.14.7 || >=15.0.0"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "axios": "^0.16.0",
    "esrever": "^0.2.0",
    "format-message": "^5.1.2",
    "format-message-parse": "^5.1.2",
    "format-message-print": "^5.1.2"
  }
}
